<%- include ('partials/header.ejs') %>
<%- include ('partials/title.ejs', {title: category.title }) %>

<% category.articles?.forEach((article) => { %>
  <div class="card mb-2">
    <div class="card-header">
      <%= article.title %>
    </div>
    <div class="card-body">
      <a href="/article/<%= article.slug %>" class="btn btn-outline-primary">Read article</a>
    </div>
  </div>
<% }); %>

<%- include ('partials/footer.ejs') %>