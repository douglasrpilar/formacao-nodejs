<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <meta http-equiv="X-UA-Compatible" content="ie=edge">
  <title>Table</title>
</head>
<body>
  <table>
    <thead>
      <tr>
        <% header.forEach((col) => { %>
          <th><%= col %></th>
        <% }) %>
      </tr>
    </thead>
    <tbody>
      <% rows.forEach((row) => { %>
        <tr>
          <% row.forEach((col) => { %>
            <td><%= col %></td>
          <% }) %>
        </tr>
      <% }) %>
    </tbody>
  </table>
</body>
</html>